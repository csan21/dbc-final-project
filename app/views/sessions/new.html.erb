<div class="login-page">
  <div class="form">
    <%= form_for :session, url: '/sessions' do |f| %>
      <%= f.text_field :email, placeholder: "email" %><br>
      <%= f.password_field :password, placeholder: "password" %><br>
      <%= f.submit "LOGIN", class: "button" %>
    <% end %>
    <p class="message">Not on Squad?<a href="/sessions/new"> Sign up</a></p>
  </div>
</div>
